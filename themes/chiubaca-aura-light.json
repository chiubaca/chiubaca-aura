  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#8c959f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant", "constant.character", "constant.language", "constant.numeric", "constant.other"],
      "settings": {
        "foreground": "#0969da"
      }
    },
    {
      "scope": ["entity", "entity.name", "entity.name.class", "entity.name.function", "entity.name.method", "entity.name.namespace", "entity.name.tag", "entity.name.type"],
      "settings": {
        "foreground": "#b87a00"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal", "invalid.deprecated"],
      "settings": {
        "foreground": "#cf222e",
        "fontStyle": ""
      }
    },
    {
      "scope": ["keyword", "keyword.control", "keyword.operator", "keyword.other"],
      "settings": {
        "foreground": "#cf222e"
      }
    },
    {
      "scope": ["markup", "markup.bold", "markup.heading", "markup.italic", "markup.list", "markup.quote", "markup.raw", "markup.underline"],
      "settings": {
        "foreground": "#24292f"
      }
    },
    {
      "scope": ["meta", "meta.block", "meta.function", "meta.function-call", "meta.property-name", "meta.return-type", "meta.selector"],
      "settings": {
        "foreground": "#24292f"
      }
    },
    {
      "scope": ["storage", "storage.type"],
      "settings": {
        "foreground": "#cf222e"
      }
    },
    {
      "scope": ["string", "string.html", "string.markdown", "string.quoted", "string.unquoted"],
      "settings": {
        "foreground": "#1a7f37"
      }
    },
    {
      "scope": ["support", "support.class", "support.constant", "support.function", "support.type"],
      "settings": {
        "foreground": "#0969da"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#24292f"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#24292f"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#bf8700"
      }
    },
    {
      "name": "Amber accent for functions and classes",
      "scope": ["entity.name.function", "entity.name.type"],
      "settings": {
        "foreground": "#b87a00"
      }
    },
    {
      "name": "Blue for numbers and constants",
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#0969da"
      }
    },
    {
      "name": "Green for strings",
      "scope": "string",
      "settings": {
        "foreground": "#1a7f37"
      }
    },
    {
      "name": "Red for keywords and operators",
      "scope": ["keyword", "storage.type", "punctuation"],
      "settings": {
        "foreground": "#cf222e"
      }
    },
    {
      "name": "Purple for decorators",
      "scope": "entity.name.decorator",
      "settings": {
        "foreground": "#8250df"
      }
    },
    {
      "name": "Amber for tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#b87a00"
      }
    },
    {
      "name": "Attributes in tags",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#0969da"
      }
    }
  ]
}
