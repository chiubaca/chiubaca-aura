  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#484f58",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant",
        "constant.character",
        "constant.language",
        "constant.numeric",
        "constant.other"
      ],
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "scope": [
        "entity",
        "entity.name",
        "entity.name.class",
        "entity.name.function",
        "entity.name.method",
        "entity.name.namespace",
        "entity.name.tag",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#f0a500"
      }
    },
    {
      "scope": [
        "invalid",
        "invalid.illegal",
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#ff7b72",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#ff7b72"
      }
    },
    {
      "scope": [
        "markup",
        "markup.bold",
        "markup.heading",
        "markup.italic",
        "markup.list",
        "markup.quote",
        "markup.raw",
        "markup.underline"
      ],
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": [
        "meta",
        "meta.block",
        "meta.function",
        "meta.function-call",
        "meta.property-name",
        "meta.return-type",
        "meta.selector"
      ],
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#ff7b72"
      }
    },
    {
      "scope": [
        "string",
        "string.html",
        "string.markdown",
        "string.quoted",
        "string.unquoted"
      ],
      "settings": {
        "foreground": "#7ee787"
      }
    },
    {
      "scope": [
        "support",
        "support.class",
        "support.constant",
        "support.function",
        "support.type"
      ],
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#ffa657"
      }
    },
    {
      "name": "Amber accent for functions and classes",
      "scope": [
        "entity.name.function",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#f0a500"
      }
    },
    {
      "name": "Midnight blue for numbers and constants",
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "name": "Soft green for strings",
      "scope": "string",
      "settings": {
        "foreground": "#7ee787"
      }
    },
    {
      "name": "Coral red for keywords and operators",
      "scope": [
        "keyword",
        "storage.type",
        "punctuation"
      ],
      "settings": {
        "foreground": "#ff7b72"
      }
    },
    {
      "name": "Lavender for decorators",
      "scope": "entity.name.decorator",
      "settings": {
        "foreground": "#d2a8ff"
      }
    },
    {
      "name": "Amber for tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#f0a500"
      }
    },
    {
      "name": "Attributes in tags",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#79c0ff"
      }
    }
  ]
}
